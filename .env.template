DATABASE_URL="postgres://postgres.[YOUR-PROJECT-ID]:[YOUR-DB-PASSWORD]@aws-1-sa-east-1.pooler.supabase.com:6543/postgres?sslmode=require"
POSTGRES_URL="postgres://postgres.[YOUR-PROJECT-ID]:[YOUR-DB-PASSWORD]@aws-1-sa-east-1.pooler.supabase.com:6543/postgres?sslmode=require"

# Supabase Service Keys
SUPABASE_SERVICE_ROLE_KEY="[YOUR-SERVICE-ROLE-KEY]"
